<?xml version="1.0" encoding="GBK"?>
<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns="http://www.springframework.org/schema/beans"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans-4.0.xsd">

    <bean id="dogTemplate" class="com.pengfei.spring.ceateBean.AbstractBean.Dog" abstract="true">
        <constructor-arg value="4"/>
    </bean>

    <bean id="whiteDog" parent="dogTemplate" >
        <constructor-arg value="kili"/>
    </bean>

    <bean id="blackDog" parent="dogTemplate" >
        <constructor-arg value="phili"/>
    </bean>
</beans>